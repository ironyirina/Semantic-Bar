<Window x:Class="ConsultingWindow.WmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WmWindow" Height="300" Width="300" Loaded="WindowLoaded1">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ListBox Name="lbNodes" ItemsSource="{Binding}" Margin="5" BorderBrush="Blue" BorderThickness="2"></ListBox>
        <ListBox Name="lbArcs" Grid.Column="1" ItemsSource="{Binding}" Margin="5" BorderBrush="Blue" BorderThickness="2"></ListBox>
    </Grid>
</Window>
